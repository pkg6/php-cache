{
  "name": "pkg6/cache",
  "license": "MIT",
  "description": "A cache that is more comfortable to use for Chinese people",
  "keywords": [
    "cache",
    "easy-cache",
    "think-cache",
    "cache-file",
    "cache-redis",
    "cache-memcache",
    "cache-memcached",
    "cache-wincache"
  ],
  "authors": [
    {
      "name": "zzqqw",
      "email": "zhiqiangwang@aliyun.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "psr/cache": "~1.0",
    "psr/simple-cache": "^1.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Pkg6\\Cache\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6|^7|^8|^9"
  },
  "autoload-dev": {
    "psr-4": {
      "Pkg6\\Cache\\Tests\\": "tests/"
    }
  }
}
